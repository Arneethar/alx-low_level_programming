#include "holberton.h"

/**
 * main - Prints "Holberton" using putchar
 * follwed by a new line
 * Return: 0
 */
int main(void)
{
	_putchar('H');
	_putchar('0');
	_putchar('l');
	_putchar('b');
	_putchar('e');
	_putchar('r');
	_putchar('t');
	_putchar('o');
	_putchar('n');
	_putchar('\n');

	return (0);
}
